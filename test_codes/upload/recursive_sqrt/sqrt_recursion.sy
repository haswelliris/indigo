int mysqrt(int a, int x0)
{
    int x1;
    x1 = (x0 + a / x0) / 2;
    if (x1 - x0 > 1 || x1 - x0 < -1)
        return mysqrt(a, x1);
    else
        return x1;
}
int main()
{
    int x;
    int i = 0;
    while (i < 10)
    {
        x = getint();
        putint(mysqrt(x, 1));
        i = i + 1;
    }
    return 0;
}