
int n;
int a[10];
int v[10];

void DFS(int p)
{
	int i=0;
	if(p==n){//?? 
		putarray(n, a);
		putch(10);
		return;
	}
	while (i<n) { //??0->n-1(???1->n)??? v[i]??????????? 
		if(v[i]==1) {
			i=i+1;
			continue;
		}
		v[i]=1;
		a[p]=i;//p???? ????????? 
		DFS(p+1);
		v[i]=0;//?? 
		i = i+1;
	}
}

int main()
{
	int i;
	while(1){
		n = getint();
		if(n==0) {
			break;
		}
		i=0;
		while (i<10){
			v[i]=0;
			i=i+1;
		}
		DFS(0);
	}
	return 0;
}
